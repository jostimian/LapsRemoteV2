<mah:MetroWindow x:Class="LapsRemote.Screens.Main"
		xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
		xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LapsRemote.Screens"
        mc:Ignorable="d"
		Title="LRC" Height="720" Width="1280">
	<mah:MetroWindow.RightWindowCommands>
		<mah:WindowCommands>
			<Button ToolTip="Open Github Repo" Click="OpenRepo">
				<StackPanel Orientation="Horizontal">
					<iconPacks:PackIconMaterial VerticalAlignment="Center" Kind="Github" Width="20" Height="20"/>
					<TextBlock VerticalAlignment="Center" Text="Contribute" Margin="4 0 0 0"></TextBlock>
				</StackPanel>
			</Button>
		</mah:WindowCommands>
	</mah:MetroWindow.RightWindowCommands>
</mah:MetroWindow>
 